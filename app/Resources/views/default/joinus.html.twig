{% trans_default_domain 'Front' %}
{% extends 'base.html.twig' %}
{% block title %}{{ 'website.title'|trans }} | {{ 'website.joinus-title'|trans }}{% endblock %}
{% block description %}{{ text_block('repertoire_old_intro', true) }}{% endblock %}
{% block opengraph %}
<meta property="og:title" content="{{ 'website.joinus-title'|trans }}" />
<meta property="og:description" content="{{ 'website.joinus-content'|trans|striptags|truncate(150, true) }}" />
<meta property="og:type" content="website" />
<meta property="og:url" content="{{ absolute_url(path('joinus')) }}" />
<meta property="og:image" content="{{ absolute_url(asset('images/og-default.jpg')) }}" />
{% endblock %}
{% block body %}
<div class="column row">
     <h1>{{ 'website.joinus-title'|trans }}</h1>
     <div>{{ text_block('nous_rejoindre_contenu') }}</div>
</div>
{% endblock %}
