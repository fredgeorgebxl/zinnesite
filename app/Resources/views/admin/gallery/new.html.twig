{% trans_default_domain 'App' %}
{% extends "admin/base-admin.html.twig" %}
{% form_theme form 'bootstrap_3_layout.html.twig' %}
{% block title %}{{'general.administration'|trans}} | {{'general.add'|trans}} {{'gallery.gallery'|trans}}{% endblock %}
{% block body %}
    <h1>{{'general.add'|trans}} {{'gallery.gallery'|trans}}</h1>
    {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
        {{ form_row(form.title) }}
        {{ form_row(form.save) }}
    {{ form_end(form) }}
{% endblock %}